    "from sklearn.preprocessing import LabelEncoder \n",
    "file = '/Users/jessiding/Downloads/SIData2022.csv'\n",
    "df = pd.read_csv(file)\n",
    "le=LabelEncoder()\n",
    "df['label']=le.fit_transform(df['label'])"
   ]
